{

    "manifest_version": 2,
    "name": "Darkify",
    "version": "1.0",
  
    "description": "Adds a black filter to all websites.",
  
    "icons": {
      "48": "icon.svg",
      "96": "icon.svg"
    },
  
    "content_scripts": [
      {
        "matches": ["*://*/*"],
        "js": ["darkify.js"]
      }
    ]
  
  }